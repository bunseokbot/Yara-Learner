        function playMovie() {
            if("N" == CONSTANT_PC) {	//	pc 버전일 경우 동영상 재생할 수 있도록 하기
                $("#DaumVodPlayer").width($("#daumVodPlayerImg").width());
                $("#DaumVodPlayer").height($("#daumVodPlayerImg").height());

                $("#daumVodPlayerImg").hide();
                $("#daumVodPlayerBox").show();

                var vod = $('#daumVodPlayer').contentWindow;
                vod.daumVodPlayerVideoPlay(true);
            }
            else {
                $("#DaumVodPlayer").width($("#daumVodPlayerImg").width());
                $("#DaumVodPlayer").height($("#daumVodPlayerImg").width()*0.5);

                $("#daumVodPlayerImg").hide();
                $("#daumVodPlayerBox").show();

                var vod = $('#daumVodPlayer').contentWindow;
                vod.daumVodPlayerVideoPlay(true);
            }
        }

        function shufflePeople() {

            var array = [
                {
                    image : "../images/operating/pc_David.png",
                    m_image : "../images/operating_m/m_David.png",
                    text : "언제나 연결되어 있는<br>당신의 친구,<br>카카오가 될께요.",
                    m_text : "언제나 연결되어 있는<br>당신의 친구,<br>카카오가 될께요.",
                    name : "David"
                },
                {
                    image : "../images/operating/pc_Deborah.png",
                    m_image : "../images/operating_m/m_Deborah.png",
                    text : "관계는 더 돈독하게,<br>생활은 더 편리하게,<br>세상은 더 즐겁게, 카카오.",
                    m_text : "관계는 더 돈독하게,<br>생활은 더 편리하게,<br>세상은 더 즐겁게, 카카오.",
                    name : "Deborah"
                },
                {
                    image : "../images/operating/pc_Flynn.png",
                    m_image : "../images/operating_m/m_Flynn.png",
                    text : "너, 나 그리고 우리가<br>만들어 나가는 새로운 경험<br>카카오가 함께합니다.",
                    m_text : "너, 나 그리고 우리가<br>만들어 나가는 새로운 경험<br>카카오가 함께합니다.",
                    name : "Flynn"
                },
                {
                    image : "../images/operating/pc_Hobbes.png",
                    m_image : "../images/operating_m/m_Hobbes.png",
                    text : "카카오가 만드는 연결된 세상,<br>당신의 일상을<br>더욱 특별하게 만듭니다.",
                    m_text : "카카오가 만드는 연결된 세상,<br>당신의 일상을<br>더욱 특별하게 만듭니다.",
                    name : "Hobbes"
                }
            ];

            var currentIndex = array.length, temporaryValue, randomIndex;

            // While there remain elements to shuffle...
            while (0 !== currentIndex) {

                // Pick a remaining element...
                randomIndex = Math.floor(Math.random() * currentIndex);
                currentIndex -= 1;

                // And swap it with the current element.
                temporaryValue = array[currentIndex];
                array[currentIndex] = array[randomIndex];
                array[randomIndex] = temporaryValue;
            }

            return array;
        }

        function createPcSlideNode() {	//	pc 슬라이드 node 생성하는 구문
            var pcWrapSlideNode = $("#pcWrapSlideNode");

            var randomArray = shufflePeople();
            for(var i = 0 ; i < 4 ; i++) {

                var dataNode = randomArray[i]

                if(i == 0) {
                    pcWrapSlideNode.append(
                            '<div class="slide_people">' +
                            '	<img src="' + dataNode.image + '" class="img_people" alt >' +
                            '	<div class="cont_img">' +
                            '		<p class="desc_box">' + dataNode.text + '</p>' +
                            '		<span class="txt_name">' + dataNode.name + '</span>' +
                            '	</div>' +
                            '</div>'
                    );

                    $(".slide_people").find("img").one('load', function() {	//	첫번째 이미지 로딩이 끝나면 해당 전체 노드 resize 함수 실행
                        resizeNode();
                    });
                } else {
                    pcWrapSlideNode.append(
                            '<div class="slide_people slide_type' + (i + 1) + '">' +
                            '	<img src="' + dataNode.image + '" class="img_people" alt >' +
                            '	<div class="cont_img">' +
                            '		<p class="desc_box">' + dataNode.text + '</p>' +
                            '		<span class="txt_name">' + dataNode.name + '</span>' +
                            '	</div>' +
                            '</div>'
                    );
                }
            }
        }

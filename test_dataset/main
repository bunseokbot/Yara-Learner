        function createCookie(name,value,days) {
            if (days) {
                var date = new Date();
                date.setTime(date.getTime()+(days*24*60*60*1000));
                var expires = "; expires="+date.toGMTString();
            }
            else var expires = "";
            document.cookie = name+"="+value+expires+"; path=/";
        }

        function eraseCookie(name) {
            createCookie(name,"",-1);
        }

        function getCookie(cname){
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i=0; i<ca.length; i++){
                var c = ca[i].trim();
                if (c.indexOf(name)==0) return c.substring(name.length,c.length);
            }
            return "";
        }

		window.onload = function() {
			createPcSlideNode();
			setWindowResizeEvent();
			setDocumentScrollEvent();
			if(isMobile == CONSTANT_PC) {	//	모바일 버전이라면은 selectbox 이벤트 동작 안시키기
				setSlideEvent();
			}
			else {
				mSlide.timer = setInterval(function(){mSlide.sl.next();},5000);
				subSlide.timer = setInterval(function(){subSlide.sl.next();},5000);;

				//	main 화면에서 가로 세로 모드 변환시 동영상 부분 너비 설정
				window.addEventListener("orientationchange", function() {
					//alert($("#daumVodPlayerImg").width());

					setTimeout(function () {	//	저사양 단말일 경우 단말기 가로, 세로 전환후 바로 왼쪽메뉴를 설정한다면 이전 모드(가로, 세로) 사이즈 크기를 가지고 있어서 이 문제점을 해결하기 위해 300 초 정도 시간을 경과시킨후 왼쪽메뉴 높이 지정
						$("#DaumVodPlayer").width($("#daumVodPlayerImg").width());
	                	$("#DaumVodPlayer").height($("#daumVodPlayerImg").width()*0.5);
					}, CONSTANT_MAIN_MOVIE_SETTING_TIMEOUT);

        		}, false);
			}

            //위드카카오에서 공지사항을 보여주기 위한 팝업오픈 값 추가
            
		}

		function setWindowResizeEvent() {
			$(window).resize(function() {
				resizeNode();

				setLayerPopUpTopPosition();	//	공고사항 팝업 노출 위치 수정
			});

			resizeNode();
		}

		function resizeNode() {
			if("N" == CONSTANT_MOBILE) {	//	mobile 화면 이라면 모바일 화면 전용 resize 코드 타기
			}
			else {									//	pc 화면 이라면 모바일 화면 전용 resize 코드 타기
				var pcPeopleNode = $("#pcPeopleNode");
				var pcWrapSlideNode = $("#pcWrapSlideNode");
				pcPeopleNode.height(pcWrapSlideNode.height());


				// 공지사항 레이아웃 높이 등록하기
				if($("#daumVodPlayerImg").height() + $(".main_type2").height() > $(".cont_sub").height()) {
					$(".main_type1").height( $(".main_type2").height() - Number($(".main_type1").css("padding-top").replace("px", "")));//padding-top
				}
				else {
					$(".main_type1").height( $(".cont_sub").height() - $(".daumVodPlayerImg").height() );
				}

				/*	동영상 영역 사이즈 조절하기	*/
				$("#DaumVodPlayer").width($("#daumVodPlayerBox").width());
				$("#DaumVodPlayer").height($("#daumVodPlayerBox").height());
			}

			// 공지사항 레이아웃 높이 등록하기
			$(".dimmed_layer").height($(document).height());
		}

		function setSlideEvent() {
			pcWrapSlideNode = $("#pcWrapSlideNode");	//	slide 되는 div 태그 select
			pcPeoplePagingNode = $("#pcPeoplePagingNode");	//	slide 되는 div 태그내에 있는 PagingNode select

			pcWrapSlideNode.children().each(function(i, val) {	//	selectboxOption 내에 있는 li 요소들 select 해서 각각요소에 click 이벤트 주기
        		$(val).attr("indexNum", i);
        	});

			slideTimer = setInterval(autoSlide, INTERVALTIME);

			$("#pcPeopleNodePrev").click(function(event){
				if($(this).attr("doAnimation") == "true") {
					return;
				}

				$(this).attr("doAnimation", true);

				clearInterval(slideTimer);
				moveSlide(true);
			});

			$("#pcPeopleNodeNext").click(function(event){
				if($(this).attr("doAnimation") == "true") {
					return;
				}

				$(this).attr("doAnimation", true);

				clearInterval(slideTimer);
				moveSlide();
			});

			// slide node mouse hover event 추가
			pcWrapSlideNode.hover(
				function(event){
					clearInterval(slideTimer);
				},
				function(event){
					slideTimer = setInterval(autoSlide, INTERVALTIME);
			});

			// next button mouse hover 이벤트 추가
			$("#pcPeopleNodeNext").children().hover(
				function(event){
					clearInterval(slideTimer);
				},
				function(event){
					slideTimer = setInterval(autoSlide, INTERVALTIME);
				}
			);

			// prev button mouse hover 이벤트 추가
			$("#pcPeopleNodePrev").children().hover(
				function(event){
					clearInterval(slideTimer);
				},
				function(event){
					slideTimer = setInterval(autoSlide, INTERVALTIME);
				}
			);
		}

		function moveSlide(isRight) {
			if(!isRight) {	//	next 오른쪽 버튼 클릭시
    			var left = pcWrapSlideNode.children().outerWidth() * -1;

    			pcWrapSlideNode.animate({'left': left },'normal', function(){
					//	slide 된 영역 변경
    				pcWrapSlideNode.css("left",0);

    				var firstNode = pcWrapSlideNode.children()[0];
    				var cloneNode = firstNode.cloneNode(true);
    				$(firstNode).remove();
    				pcWrapSlideNode.append(cloneNode);

					$("#pcPeopleNodeNext").attr("doAnimation", false);

					currentIndex = $(pcWrapSlideNode.children()[0]).attr("indexNum");	//	현재 보고 있는 요소의 index number 추출
					updatePagingView();	//	slide 된 후 pagingView 업데이트 하는 함수
    			});
			}
			else {	//	prev 왼쪾 버튼 클릭시

				//	slide 된 영역 변경
    			var left = pcWrapSlideNode.children().outerWidth() * -1;

				var firstNode = pcWrapSlideNode.children()[pcWrapSlideNode.children().length -1];
				var cloneNode = firstNode.cloneNode(true);
				$(firstNode).remove();
				pcWrapSlideNode.prepend(cloneNode);
				pcWrapSlideNode.css("left",left);

				pcWrapSlideNode.animate({'left': 0 },'normal', function(){
					$("#pcPeopleNodePrev").attr("doAnimation", false);

					currentIndex = $(pcWrapSlideNode.children()[0]).attr("indexNum");	//	현재 보고 있는 요소의 index number 추출
					updatePagingView();	//	slide 된 후 pagingView 업데이트 하는 함수
    			});
			}
		}

		//	slide 된 후 pagingView 업데이트 하는 함수
		function updatePagingView() {
			var childrenNode = pcWrapSlideNode.children();
			//currentIndex
			$("#pcPeoplePagingNode").html("");
			for(var i = 0 ; i < childrenNode.length ; i++) {
				if(currentIndex == i) {
					$("#pcPeoplePagingNode").append('<span class="screen_out">현재페이지</span>');
					$("#pcPeoplePagingNode").append('<em class="num_page">' + (i+1) + '</em>');
				}
				else {
					$("#pcPeoplePagingNode").append('<span class="num_page">' + (i+1) + '</span>');
				}
			}
		}

		function autoSlide(){
			moveSlide();
		}

		function openAnnouncementLayout(id) {
			if("N" == CONSTANT_MOBILE) {	//	모바일 버전이라면 새 화면 open
				var url = "/etc/announcePopup?noticeId="+ id;
    			var name = "개정 전 개인정보 처리방침";
    			var w = "800px";
    			var h = "700px";
    			var scroll = "yes";
    			popCwin(url,name,w,h,scroll);
			}
			else {
    			$(".dimmed_layer").show();
    			$("#notice" + id).show();

    			setLayerPopUpTopPosition();
			}
		}

		function closeAnnouncementLayout() {
			$(".dimmed_layer").hide();
			$(".layer_corp").hide();
		}



		//스크롤 관련 액션
		var scrollTimeout = null;
		function setDocumentScrollEvent() {//left menu 최하단 배너 영역 move 이벤트 추가
        	$(window).scroll(function() {
        	    if ( scrollTimeout === null ) {
        	        //scrollbeginHandler();
        	    } else {
        	        clearTimeout( scrollTimeout );
        	    }
        	    scrollTimeout = setTimeout( setLayerPopUpTopPosition, 500 );
        	});
        }
        //popUplayer top position 위치 지정해주는 공통함수
        function setLayerPopUpTopPosition() {
            if("N" == CONSTANT_MOBILE) {	//	모바일 화면이면 모바일 관련 기능 수행
                var rmLayerList = $(".layer_corp");//로그인, 회의실 예약, 빈회의실 검색 popupLayer select
                for(var i = 0 ; i < rmLayerList.length; i++) {
                    var rmLayer = $(rmLayerList[i]);

                    var layerHeight = rmLayer.height();
                    var currentScrollTop = window.pageYOffset || document.documentElement.scrollTop;
                    var clientHeight = document.documentElement.clientHeight / 2;
                    var topPosition = clientHeight - (layerHeight / 2) + currentScrollTop;
                    rmLayer.css("top", topPosition);
                }
            }
            else {	//	pc 화면이면 pc 화면 기능 수행
                var rmLayerList = $(".layer_corp");//로그인, 회의실 예약, 빈회의실 검색 popupLayer select
                for(var i = 0 ; i < rmLayerList.length; i++) {
                    var rmLayer = $(rmLayerList[i]);

                    // 화면 사이즈가 700 이하일 경우 popuplayout height 400으로 설정

                    if(document.documentElement.clientHeight < 700) {
                        $(".layer_body ").height(400);
                    }
                    else {
                        $(".layer_body ").height(597);
                    }

                    var layerHeight = rmLayer.height();
                    var currentScrollTop = window.pageYOffset || document.documentElement.scrollTop;
                    var clientHeight = document.documentElement.clientHeight / 2;
                    var topPosition = clientHeight - (layerHeight / 2) + currentScrollTop;
                    rmLayer.css("top", topPosition);


                    var clientWidth = document.documentElement.clientWidth / 2;
                    var layerWidth = $(".inner_corp_layer").width();
                    var leftPosition = clientWidth - (layerWidth / 2);
                    rmLayer.css("left", leftPosition);

                    // 스크롤 바 표시여부 하는 구문
                    rmLayer.find(".layer_body").removeClass("scroll_g");

                    if(rmLayer.find(".notice_cont").height() > $(".layer_body ").height()) {
                        rmLayer.find(".layer_body").addClass("scroll_g");
                    }

                    // 화면사이즈마다 dimmed 창 width가 다르기 때문에 popup이 호출될때마다 dimmed 창 width 조절하는 코드 추가
                    $(".dimmed_layer").width($(".inner_daumkakaowrap").width());
                }
            }
        }

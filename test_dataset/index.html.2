function getToday(fromDate, toDate) {

 //fromDate, toDate 입력형식

 //[YYYY][MM][DD][HH][MM][SS] - 14자리로 년월일시분초까지 입력 136번

 today = new Date();

 var yy = String(today.getFullYear());

 var mm = today.getMonth() + 1; if ( String(mm).length == 1){mm = '0'+String(mm);}

 var dd = today.getDate();if ( String(dd).length == 1){dd = '0'+String(dd);}

 var minute = today.getMinutes();if ( String(minute).length == 1){minute = '0'+String(minute);}

 var second = today.getSeconds();if ( String(second).length == 1){second = '0'+String(second);}

 var hour = today.getHours();if ( String(hour).length == 1){hour = '0'+String(hour);}

 //document.write("오늘은 "+yy+mm+dd+hour+minute+second+"<br>");



 var todayDate = yy+mm+dd+hour+minute+second;

// alert("오늘은 "+todayDate+"/"+fromDate+"/"+toDate);

 if ( todayDate > fromDate && todayDate < toDate ) { return true; }

 else { return false; }

}



function setCookie( name, value, expiredays ){

        var todayDate = new Date();

        todayDate.setDate( todayDate.getDate() + expiredays );

        document.cookie = name + "=" + escape( value ) + "; path=/;domain=www9.sejong.ac.kr; expires=" + todayDate.toGMTString() + ";"

}



function getCookie( name )

{

        var nameOfCookie = name + "=";

        var x = 0;

        while ( x <= document.cookie.length )

        {

                var y = (x+nameOfCookie.length);

                if ( document.cookie.substring( x, y ) == nameOfCookie ) {

                        if ( (endOfCookie=document.cookie.indexOf( ";", y )) == -1 )

                                endOfCookie = document.cookie.length;

                        return unescape( document.cookie.substring( y, endOfCookie ) );

                }

                x = document.cookie.indexOf( " ", x ) + 1;

                if ( x == 0 )

                        break;

        }

        return "";

}


            $('#cve' + count + '_detec').html('<code>'+obj.data.Exploited_Vulnerabilities[count].Detection_Name+'</code>');
            $('#cve' + count + '_platform').html(obj.data.Exploited_Vulnerabilities[count].Target_Platform);
            $('#cve' + count + '_count').html(obj.data.Exploited_Vulnerabilities[count].count);
        }
        for(count=0; count<obj.data.list.length; count++) {
            if(count == 8) {
                break;
            }
            if(obj.data.list[count].pattern.indexOf("mal.phishing") > -1) {
                document.getElementById("uri"+count).setAttribute('onclick', 'window.open("' + window.location.origin + '/report?type=uri&idx='+obj.data.list[count].idx+'&target=b")');
            }
            else if(obj.data.list[count].pattern.indexOf("mal.ransome") > -1) {
                document.getElementById("uri"+count).setAttribute('onclick', 'window.open("' + window.location.origin + '/report?type=uri&idx='+obj.data.list[count].idx+'&target=b")');
            }
            else if(obj.data.list[count].pattern.indexOf("mal.history") > -1) {
                document.getElementById("uri"+count).setAttribute('onclick', 'window.open("' + window.location.origin + '/report?type=uri&idx='+obj.data.list[count].idx+'&target=d")');
            }
            else if(obj.data.list[count].pattern.indexOf("mal.statcount") > -1) {
                document.getElementById("uri"+count).setAttribute('onclick', 'window.open("' + window.location.origin + '/report?type=uri&idx='+obj.data.list[count].idx+'&target=d")');
            }
            else {
                document.getElementById("uri"+count).setAttribute('onclick', 'window.open("' + window.location.origin + '/report?type=uri&idx='+obj.data.list[count].idx+'&target=a")');
            }
            $("#uri"+count+"_time").html(timestampToReadable(obj.data.list[count].timestamp));
            $("#uri"+count+"_uri").html(obj.data.list[count].uri);
            $("#uri"+count+"_ip").html(obj.data.list[count].ip);
            $("#uri"+count+"_country").html('<span class="flag-icon flag-icon-'+obj.data.list[count].ip_country.toLowerCase()+'"></span>'+obj.data.list[count].ip_country);
            document.getElementById("uri"+count+"_country").setAttribute('data-original-title', getCountryName(obj.data.list[count].ip_country));
        };

        for(count=0; count<obj.data.BADWAREINFOPattern.length; count++) {
            if(count == 7) {
                break;
            }
            $("#pattern"+count+"_detec").html('<code>'+obj.data.BADWAREINFOPattern[count].pattern+'</code>');
            $("#pattern"+count+"_count").html(obj.data.BADWAREINFOPattern[count].count);
        };
    };

    var stop=false;
    var obj;
    var createDOM = function(){
        if(stop){
            return;
        }
        $.get('https://beta.badware.info:8443/api/dashboard?uuid=' + getUUID4() + '&captcha=' + getcaptcha(), function(parseData){
            obj = JSON.parse(parseData);
            if(obj.status === 401){
                popupcaptcha();
                stop = true;
                return;
            }
            else if(obj.status !== 200) {
                location.href="./404";
            }
            _createDOM(obj);
        })
    };
    createDOM();
    $(window).resize(function(){_createDOM(obj)});
    setInterval(createDOM, 1000 * 10);
</script>
</body>
</html>
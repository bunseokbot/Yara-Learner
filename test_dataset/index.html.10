            document.getElementById("av"+count).setAttribute('data-original-title', getAVName(data[count].Anti_virus_Name));
            $('#av' + count + '_detec').html(data[count].Detection_Name);
            $('#av' + count + '_img').html('<img src="static/img/'+getAVImg(data[count].Anti_virus_Name)+'" alt="'+getAVName(data[count].Anti_virus_Name)+'" title="'+getAVName(data[count].Anti_virus_Name)+'" />');
            $('#av' + count + '_count').html(data[count].count);
        };


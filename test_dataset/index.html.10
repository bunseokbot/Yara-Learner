<script>
    $("#ihatejavascript")[0].value = getUUID4();
    var _createDOM = function (obj) {
        var today = new Date();
        var day = today.getDate();
        var month_year = getMonthName(timestampToReadable(obj.data.list[0].timestamp).slice(5, 7))+', '+timestampToReadable(obj.data.list[0].timestamp).slice(0, 4);
        var month = getMonthName(timestampToReadable(obj.data.list[0].timestamp).slice(5, 7));
        $("#month1").html(month);
        $("#month2").html(month);
        $("#month3").html(month);
        $("#month_year").html(month_year);
        $("#malicious_uris").html(obj.data.Malicious_URIs);
        $("#malicious_ransom").html(obj.data.RansomwareSites);
        $("#malicious_phish").html(obj.data.PhishingSites);
        $("#malicious_host").html(obj.data.Malicious_Hostnames);
        $("#malicious_ip").html(obj.data.Malicious_IPs);
        $("#malicious_file").html(obj.data.Malicious_Files);

        var data = obj.data.Most_Infected_Country;
        for(count=0; count<data.length; count++) {
            if(count == 8) {
                break;
            }
            document.getElementById("country"+count).setAttribute('onclick', 'window.open("' + window.location.origin + '/report?name='+data[count].country+'&target=c")');
            $('#country' + count + '_ctr').html('<span class="flag-icon flag-icon-' + data[count].country.toLowerCase() + '"></span>' + getCountryName(data[count].country) + '');
            $('#country' + count + '_count').html(data[count].count);
            $('.spark_14days_' + count + '').sparkline(data[count].last14, {
                type: 'line',
                minSpotColor: '#e7304a',
                maxSpotColor: '#e7304a',
                lineColor: '#fff',
                lineWidth: 2,
                spotRadius: 2,
                fillColor: '#404652',
                spotColor: '#fff',
                highlightSpotColor: '#e7304a',
                highlightLineColor: '#e7304a',
                width: '100%',
                height: '20'
            });
        };

        var Malicious_URIs_last30 = movegraphleft(obj.data.Malicious_URIs_last30, day);
        $(".spark_uris").sparkline(Malicious_URIs_last30, {
            type: 'line',
            minSpotColor: '#e7304a',
            maxSpotColor: '#e7304a',
            lineColor: '#fff',
            lineWidth: 3,
            spotRadius: 3,
            fillColor: '#404652',
            spotColor: '#fff',
            highlightSpotColor: '#e7304a',
            highlightLineColor: '#e7304a',
            width: '100%',
            height: '79'
        });
        var Malicious_Files_last30 = movegraphleft(obj.data.Malicious_Files_last30, day);
        $(".spark_files").sparkline(Malicious_Files_last30, {
            type: 'line',
            minSpotColor: '#e7304a',
            maxSpotColor: '#e7304a',
            lineColor: '#fff',
            lineWidth: 3,
            spotRadius: 3,
            fillColor: '#404652',
            spotColor: '#fff',
            highlightSpotColor: '#e7304a',
            highlightLineColor: '#e7304a',
            width: '100%',
            height: '79'
        });

        var data = obj.data.AV_Detection_Details;
        for(count=0; count < data.length; count++) {
            if(count == 7) {
                break;
